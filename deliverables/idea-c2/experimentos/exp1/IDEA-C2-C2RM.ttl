@base <http://idea-c2.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix c2rm: <http://idea-c2.org/c2rm/#> .
@prefix cnt: <http://idea-c2.org/data/#> .


c2rm:entity a rdfs:Class .

c2rm:relation
  a rdf:Property ;
  rdfs:domain c2rm:entity ;
  rdfs:range c2rm:entity .

c2rm:generaldomainrelation
    a rdf:Property;
    rdfs:subPropertyOf c2rm:relation.

c2rm:c2domainrelation
    a rdf:Property;
    rdfs:subPropertyOf c2rm:relation.
	

c2rm:equivalent_to
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .

  
c2rm:associated_with
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .


c2rm:composed_of
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .


c2rm:type_of
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .


c2rm:defined_by
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .

c2rm:co-referenced
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .

c2rm:instance_of
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:generaldomainrelation .
  

c2rm:responsible_for
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:c2domainrelation .
 

c2rm:capacity_of
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:c2domainrelation .
 
  
c2rm:occurs_in
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:c2domainrelation .
 

c2rm:applied_to
  a rdf:Property ;
  rdfs:subPropertyOf c2rm:c2domainrelation .
